    "bad-words": "^4.0.0",
    "gsap": "^3.13.0",
    "lucide-react": "^0.546.0",
    "next": "16.0.10",
    "next-auth": "5.0.0-beta.30",
    "react": "19.2.3",
    "react-dom": "19.2.3",
